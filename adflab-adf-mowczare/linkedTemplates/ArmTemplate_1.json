{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adflab-adf-mowczare"
		},
		"Daily - DW Load Trigger_properties_Master - Begin DW Load_parameters_recipient": {
			"type": "string",
			"defaultValue": "mateusz.owczarek@pentacomp.pl"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Daily - DW Load Trigger')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Master - Begin DW Load",
							"type": "PipelineReference"
						},
						"parameters": {
							"recipient": "[parameters('Daily - DW Load Trigger_properties_Master - Begin DW Load_parameters_recipient')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Day",
						"interval": 1,
						"startTime": "2020-06-03T09:26:00.000Z",
						"timeZone": "UTC",
						"schedule": {
							"hours": [
								10
							]
						}
					}
				}
			},
			"dependsOn": []
		}
	]
}